{
  "name": "blackwall",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --bun vite dev --port 3000",
    "build": "APP_SECRET=some-key DATABASE_URL=:memory: APP_BASE_URL=http://localhost:3000 bun run --bun vite build",
    "serve": "bun run --bun vite preview",
    "test": "vitest run",
    "start": "bun dist/server/server.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "bun run --bun drizzle-kit generate",
    "db:migrate": "bun run ./scripts/migrator.ts ./src/db/migrations",
    "db:drop": "bun ./src/db/drop.ts",
    "db:seed": "bun ./src/db/seeders/index.ts"
  },
